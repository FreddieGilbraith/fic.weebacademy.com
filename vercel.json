{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Clacks-Overhead",
          "value": "GNU Terry Pratchet"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/bee.js",
      "destination": "https://cdn.splitbee.io/sb.js"
    },
    {
      "source": "/_hive/:slug",
      "destination": "https://hive.splitbee.io/:slug"
    }
  ]
}